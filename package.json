{
  "name": "salviascript",
  "version": "0.1.4",
  "description": "Some small utility functions",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "license": "Apache-2.0",
  "keywords": ["random", "lightweight", "typescript", "utility"],
  "scripts": {
    "dev": "nodemon --config nodemon.json src/index.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk src/index.ts",
    "start": "node build/src/index.js",
    "test": "jest",
    "test:prod": "gts lint && jest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run test:prod",
    "preversion": "npm run test:prod",
    "version": "npm run fix"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^14.11.2",
    "gts": "^3.1.0",
    "jest": "^29.0.1",
    "nodemon": "^2.0.19",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/DarkPhotonBeam/salviascript"
  }
}
